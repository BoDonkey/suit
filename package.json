{
  "name": "suitcss",
  "version": "0.6.0",
  "description": "CSS base styles, utilities, and structural components for web apps",
  "style": "index.css",
  "files": [
    "index.css"
  ],
  "dependencies": {
    "suitcss-base": "file:base",
    "suitcss-utils": "file:utils",
    "suitcss-components": "file:components"
  },
  "devDependencies": {
